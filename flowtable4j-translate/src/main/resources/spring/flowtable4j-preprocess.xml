<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
            http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
            http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-4.0.xsd">

    <context:annotation-config />

    <bean id="allTemplates"  class="com.ctrip.infosec.flowtable4j.translate.dao.Jndi.AllTemplates" init-method="init" />

    <bean id="hotelGroupSources" class="com.ctrip.infosec.flowtable4j.translate.dao.HotelGroupSources"
          depends-on="allTemplates" init-method="init">
    </bean>

    <bean id="hotelGroupWriteSources" class="com.ctrip.infosec.flowtable4j.translate.dao.HotelGroupWriteSources" init-method="init"
          depends-on="allTemplates"/>

    <bean id="tieYouSources" class="com.ctrip.infosec.flowtable4j.translate.dao.TieYouSources" depends-on="allTemplates"
          init-method="init"/>

    <bean id="tieYouWriteSources" class="com.ctrip.infosec.flowtable4j.translate.dao.TieYouWriteSources" init-method="init"
          depends-on="allTemplates"/>

    <bean id="flightSources" class="com.ctrip.infosec.flowtable4j.translate.dao.FlightSources" depends-on="allTemplates"
          init-method="init"/>

    <bean id="commonWriteSources" class="com.ctrip.infosec.flowtable4j.translate.dao.CommonWriteSources" init-method="init" depends-on="allTemplates"/>

    <bean id="commonSources" class="com.ctrip.infosec.flowtable4j.translate.dao.CommonSources"
          depends-on="allTemplates" init-method="init">
    </bean>

    <bean id="commonOperation" class="com.ctrip.infosec.flowtable4j.translate.service.CommonOperation" depends-on="commonSources"/>

    <bean id="commonExecutor" class="com.ctrip.infosec.flowtable4j.translate.service.CommonExecutor" depends-on="commonOperation"
          ></bean>

    <bean id="redisSources" class="com.ctrip.infosec.flowtable4j.translate.dao.RedisSources" init-method="init"/>

    <bean id="esbSources" class="com.ctrip.infosec.flowtable4j.translate.dao.ESBSources" init-method="init"/>

    <bean id="dataProxySources" class="com.ctrip.infosec.flowtable4j.translate.dao.DataProxySources" />


    <bean id="hotelGroupExecutor" class="com.ctrip.infosec.flowtable4j.translate.service.HotelGroupExecutor"></bean>

    <bean id = "tieYouExecutor" class="com.ctrip.infosec.flowtable4j.translate.service.TieYouExecutor"/>

    <bean id="flightExecutor" class="com.ctrip.infosec.flowtable4j.translate.service.FlightExecutor"/>

    <bean id="preProcessor" class="com.ctrip.infosec.flowtable4j.translate.PreProcessor"/>

    <!--转换器-->
    <bean id="appInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.AppInfoConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="contactInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.ContactInfoConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="corporationConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.CorporationConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="didInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.DIDInfoConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="ipInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.IPInfoConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="mainInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.MainInfoConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="paymentInfoListConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.PaymentInfoListConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="paymentMainInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.PaymentMainInfoConvert" init-method="init" depends-on="commonOperation"/>
    <bean id="userInfoConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.UserInfoConvert" init-method="init" depends-on="commonOperation"/>

    <bean id="hotelGroupConvert" class="com.ctrip.infosec.flowtable4j.translate.UnitConverters.HotelGroupConvert" init-method="init" depends-on="commonOperation"/>
</beans>